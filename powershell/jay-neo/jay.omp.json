{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "foreground": "#20022c",
          "background": "#02fddc",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "macos": "\uf179",
            "ubuntu": "\uf31b",
            "wsl": "\ue712",
            "windows": "\ue62a"
          },
          "style": "diamond",
          "template": " {{.Icon}} "
        },
        {
          "type": "shell",
          "foreground": "#20022c",
          "background": "#02fddc",
          "style": "diamond",
          "template": "{{.Name}} "
        },
        {
          "type": "root",
          "style": "diamond",
          "foreground": "#fffb38",
          "background": "#ef5350",
          "template": "<parentBackground>\ue0b0</> \uf0e7 ",
          "properties": {
            "root_icon": "\uf292"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#2d0346",
          "background": "#81f720",
          "template": " {{ .Path }} ",
          "properties": {
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#1d020f",
          "background": "#fffb38",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
            "{{ if gt .Ahead 0 }}#02fddc{{ end }}",
            "{{ if gt .Behind 0 }}#c5e478{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }} {{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "background": "#001c68",
          "foreground": "#06ffea",
          "leading_diamond": " \ue0b6",         
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }}s ",
          "type": "executiontime"
        },




        {
          "background": "#ffffff",
          "foreground": "#ce092f",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "angular"
        },
        {
          "background": "#e1e8e9",
          "foreground": "#055b9c",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "dart"
        },
        {
          "background": "#0e0e0e",
          "foreground": "#0d6da8",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue77f  {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "type": "dotnet"
        },
        {
          "background": "#06A4CE",
          "foreground": "#ffffff",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue28e {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "flutter"
        },
        {
          "background": "#ffffff",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "#ffffff",
          "foreground": "#ec2729",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "java"
        },
        {
          "background": "#945bb3",
          "foreground": "#ffffff",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "<#ca3c34>\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "julia"
        },
        {
          "background": "#906cff",
          "foreground": "#ffffff",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "K {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "kotlin"
        },
        {
          "background": "#ffffff",
          "foreground": "#000081",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue620 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "lua"
        },
        {
          "foreground": "#4ddd48",
          "background": "#303030",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\uf61a</>"
          },
          "type": "node"
        },
        {
          "background": "#41436d",
          "foreground": "#ffffff",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue769 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "perl"
        },
        {
          "background": "#787CB5",
          "foreground": "#000000",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue73d {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "php"
        },
        {
          "background": "#306998",
          "foreground": "#FFE873",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue235  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#b9bbbf",
          "foreground": "#1c68bd",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "R {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "r"
        },
        {
          "background": "#ffffff",
          "foreground": "#9c1006",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "ruby"
        },
        {
          "background": "#ffffff",
          "foreground": "#000000",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "rust"
        },
        {
          "background": "#fe562e",
          "foreground": "#ffffff",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": "\ue755 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "swift"
        },



        {
          "background": "#8f43f3",
          "foreground": "#ffffff",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "template": " \ue266 {{ round .PhysicalPercentUsed .Precision }}% ",
          "type": "sysinfo"
        },
        {
          "type": "time",
          "style": "diamond",
          "invert_powerline": true,
          "background": "#06ff83",
          "foreground": "#07119c",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "time_format": "15:04:05 "
            // "time_format": "_2 Jan | Monday 15:04:05 "
          },
          "template": " \uf5ef {{ .CurrentDate | date .Format }}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#21c7c7",
          "template": "\u2570\u2500"
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#e0f8ff",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ],
          "template": "‚ùØ",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "newline": true
    }
  ],
  "console_title_template": "{{ if .Segments.Session.SSHSession }}SSH'd{{ if or .Root }} & {{ end }}{{ end }}{{ if .Root }}# (as {{ .UserName }}) | {{ end }}{{ if .WSL }}WSL | {{ end }}{{ .Folder }} ({{ .Shell }})",
  "final_space": true,
  "osc99": true
}
